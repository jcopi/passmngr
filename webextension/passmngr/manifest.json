{
    "manifest_version": 2,
    "name": "passmngr",
    "version": "0.1",

    "description": "Federated password manager",

    "icons": {
        "48": "icons/passmngr-48.png"
    },

    "browser_action": {
        "default_icon": {
            "38": "icons/button-38.png"
        }, 
        "default_title": "passmngr",
        "default_popup": "index.html"
    },

    "background": {
        "scripts": ["core.js", "background.js"]
    }
}